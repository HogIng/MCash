        <activity android:name=".activitiy.PaymentActivity"></activity>
        <activity android:name=".activitiy.PaymentsActivity"></activity>
        <activity android:name=".activitiy.OrderActivity"></activity>
        <activity android:name=".activitiy.OrdersActivity"></activity>

    </application>

</manifest>
